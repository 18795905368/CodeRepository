<!--pages/menu/menu.wxml-->
<view class="baseview">
  <scroll-view class="scrollview" scroll-y="true">
    <view class="sview">
      <block wx:for="{{menudatas}}">
        <view class="cellview" bindtap="clickmenuObj" data-value="{{item}}">
          <view class="imgview">
            <image class="cimg" src="{{item.albums}}"></image>
          </view>
          <view class="textview">{{item.title}}</view>
        </view>
      </block>
    </view>
  </scroll-view>
</view>