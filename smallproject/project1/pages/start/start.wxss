.baseview{
  width: 100%;
  height: 100vh;
  background-color:  #F0FFFF;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tipview{
  width: 100%;
  height: 10%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.red{
  width: 150rpx;
  height: 150rpx;
  border: 2rpx solid seagreen;
  border-radius: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
  background-color:red;
}
