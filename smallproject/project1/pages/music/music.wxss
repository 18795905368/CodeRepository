/* pages/music/music.wxss */
.baseview{
  width: 100%;
  height: 100vh;
  background-color: #F8F8FF;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.cellview{
  width: 96%;
  height: 24%;
  
  margin: 5rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.imgview{
  width: 40%;
  height: 90%;
}
.cimg{
  width: 100%;
  height: 100%;
  border-radius: 5%;
}
.textview{
  width: 50%;
  height: 90%;
  
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.text1{
  width: 90%;
  height: 60%;
  font-size: 40rpx;
  font-family: 楷体;
  font-weight: bold;
  
  margin: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.text2{
  width: 90%;
  height: 20%;
  font-size: 30rpx;
  font-family: 楷体;
  
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}